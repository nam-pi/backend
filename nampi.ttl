@prefix :      <http://base/#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix tdb1:  <http://jena.hpl.hp.com/2008/tdb#> .
@prefix tdb2:  <http://jena.apache.org/2016/tdb#> .
@prefix ja:    <http://jena.hpl.hp.com/2005/11/Assembler#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix fuseki: <http://jena.apache.org/fuseki#> .
@prefix txt: <http://jena.apache.org/text#> .

tdb2:DatasetTDB  rdfs:subClassOf  ja:RDFDataset .

ja:DatasetTxnMem  rdfs:subClassOf  ja:RDFDataset .

tdb1:DatasetTDB  rdfs:subClassOf  ja:RDFDataset .

tdb1:GraphTDB   rdfs:subClassOf  ja:Model .

tdb2:GraphTDB2  rdfs:subClassOf  ja:Model .

ja:MemoryDataset  rdfs:subClassOf  ja:RDFDataset .

ja:RDFDatasetZero  rdfs:subClassOf  ja:RDFDataset .

txt:TextDataset   rdfs:subClassOf  ja:RDFDataset .

:service_tdb_all  a                        fuseki:Service ;
        rdfs:label                         "TDB2 nampi" ;
        fuseki:dataset                     :dataset ;
        fuseki:name                        "nampi" ;
        fuseki:serviceQuery                "query" , "" , "sparql" ;
        fuseki:serviceReadGraphStore       "get" ;
        fuseki:serviceReadQuads            "" ;
        fuseki:serviceReadWriteGraphStore  "data" ;
        fuseki:serviceReadWriteQuads       "" ;
        fuseki:serviceUpdate               "" , "update" ;
        fuseki:serviceUpload               "upload" .

:tdb_dataset_readwrite  a                  tdb2:DatasetTDB2 ;
        tdb2:location                      "/fuseki/databases/nampi" .

tdb2:GraphTDB  rdfs:subClassOf  ja:Model .

ja:RDFDatasetOne  rdfs:subClassOf  ja:RDFDataset .

ja:RDFDatasetSink  rdfs:subClassOf  ja:RDFDataset .

tdb2:DatasetTDB2  rdfs:subClassOf  ja:RDFDataset .


:dataset a ja:RDFDataset ;
        ja:defaultGraph :model_inf ;
        ja:namedGraph [
            ja:graph        :owlgraph ;
            ja:graphName    <https://purl.org/nampi/graph/owlgraph>
        ] ;
        ja:namedGraph [
            ja:graph        :datagraph ;
            ja:graphName    <https://purl.org/nampi/graph/datagraph>
        ] ;
        ja:namedGraph [
            ja:graph        :owlinf ;
            ja:graphName    <https://purl.org/nampi/graph/owlinf>
        ] ;
        ja:namedGraph [
            ja:graph        :datainf ;
            ja:graphName    <https://purl.org/nampi/graph/datainf>
        ] .

:owlgraph rdf:type tdb2:GraphTDB ;
        tdb2:dataset :tdb_dataset_readwrite ;
        tdb2:graphName <https://purl.org/nampi/graph/owlgraph> .

:datagraph rdf:type tdb2:GraphTDB ;
        tdb2:dataset :tdb_dataset_readwrite ;
        tdb2:graphName <https://purl.org/nampi/graph/datagraph> .

:owlinf a ja:InfModel ;
        ja:baseModel :graph ;
        ja:reasoner [
            ja:reasonerURL <http://jena.hpl.hp.com/2003/OWLFBRuleReasoner>
        ] .

:datainf a ja:InfModel ;
        ja:baseModel :graph2 ;
        ja:reasoner [
            ja:reasonerURL <http://jena.hpl.hp.com/2003/OWLFBRuleReasoner>
        ] .

:graph rdf:type tdb2:GraphTDB ;
        tdb2:dataset :tdb_dataset_readwrite ;
        tdb2:graphName <https://purl.org/nampi/graph/owlgraph> .

:graph2 rdf:type tdb2:GraphTDB ;
        tdb2:dataset :tdb_dataset_readwrite ;
        tdb2:graphName <https://purl.org/nampi/graph/datagraph> .

:model_inf a ja:InfModel ;
        ja:baseModel :union_graph ;
        ja:reasoner [
            ja:reasonerURL <http://jena.hpl.hp.com/2003/OWLFBRuleReasoner>
        ] .

:union_graph rdf:type tdb2:GraphTDB ;
        tdb2:dataset :tdb_dataset_readwrite ;
        tdb2:graphName <urn:x-arq:UnionGraph> .